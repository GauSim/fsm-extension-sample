{
  "name": "backend",
  "version": "0.0.0",
  "description": "${application_description}",
  "author": "",
  "private": true,
  "main": "main.js",
  "scripts": {
    "prebuild": "rimraf dist/*",
    "build": "tsc -p tsconfig.build.json",
    "postbuild": "npm run build:copy:static && npm run build:version && npm run build:copy:version",
    "build:copy:static": "cp-cli src/static dist/static",
    "build:version": "./src/scripts/write-version-file.sh",
    "build:copy:version": "cp-cli VERSION dist/VERSION",
    "start": "npm run start:dev",
    "prestart:dev": "npm run build:version",
    "start:dev": "nodemon --config nodemon.json",
    "start:prod:local": "cd dist && node -r dotenv/config main.js",
    "start:prod": "node main.js"
  },
  "dependencies": {
    "@nestjs/common": "^7.0.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/platform-express": "^7.0.0",
    "compression": "^1.7.4",
    "dotenv": "^8.2.0",
    "fsm-sdk": "^1.0.0-rc10",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@types/compression": "^1.7.0",
    "@types/express": "^4.17.3",
    "@types/jest": "25.1.4",
    "@types/node": "^13.9.1",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "cp-cli": "^2.0.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "jest": "^25.1.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "25.2.1",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4"
  }
}
